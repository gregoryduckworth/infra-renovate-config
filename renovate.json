{
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
 "semanticCommitScope": "deps",
  "labels": ["chore"],
  "vulnerabilityAlerts": {
    "labels": ["security"]
  },
  "docker": {
    "pinDigests": true,
    "semanticCommitType": "build"
  },
  "docker-compose": {
    "digest": {
      "enabled": false
    }
  },
  "packageRules": [
    {
      "depTypeList": ["dependencies"],
      "semanticCommitType": "build"
    }
  ]
}
